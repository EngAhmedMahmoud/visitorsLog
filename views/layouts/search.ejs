<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Ivisit | Admin Dashboad</title>
    <!-- Core CSS - Include with every page -->
    <link href="/dashboard/plugins/bootstrap/bootstrap.css" rel="stylesheet" />
    <link
      href="/dashboard/font-awesome/css/font-awesome.css"
      rel="stylesheet"
    />
    <link
      href="/dashboard/plugins/pace/pace-theme-big-counter.css"
      rel="stylesheet"
    />
    <link href="/dashboard/css/style.css" rel="stylesheet" />
    <link href="/dashboard/css/main-style.css" rel="stylesheet" />

    <!-- Page-Level CSS -->
    <link
      href="/dashboard/plugins/dataTables/dataTables.bootstrap.css"
      rel="stylesheet"
    />
  </head>
  <body>
    <!--  wrapper -->
    <div id="wrapper">
      <!-- navbar top -->
      <nav
        class="navbar navbar-default navbar-fixed-top"
        role="navigation"
        id="navbar"
        style="background:#24292e;"
      >
        <!-- navbar-header -->
        <div class="navbar-header">
          <button
            type="button"
            class="navbar-toggle"
            data-toggle="collapse"
            data-target=".sidebar-collapse"
          >
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>
        <ul class="nav navbar-top-links navbar-right" id="logout">
          <!-- end navbar-header -->
          <li class="dropdown">
            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
              <b>Admin</b>
            </a>
            <!-- dropdown user-->
            <ul class="dropdown-menu dropdown-user">
              <li>
                <a href="/admin/logout"
                  ><i class="fa fa-sign-out fa-fw"></i>Logout</a
                >
              </li>
            </ul>
            <!-- end dropdown-user -->
          </li>
        </ul>
      </nav>
      <!-- end navbar top -->

      <!-- navbar side -->
      <nav
        class="navbar-default navbar-static-side"
        role="navigation"
        style="background:#24292e;"
      >
        <!-- sidebar-collapse -->
        <div class="sidebar-collapse">
          <!-- side-menu -->
          <ul class="nav" id="side-menu" style="background:#24292e;">
            <li>
              <a href="/admin"><i class="fa fa-list fa-fw"></i><b>Logs</b></a>
            </li>
          </ul>
          <!-- end side-menu -->
        </div>
        <!-- end sidebar-collapse -->
      </nav>
      <!-- end navbar side -->
      <!--  page-wrapper -->
      <div id="page-wrapper">
        <div class="row">
          <!--  page header -->
          <div class="col-lg-12">
            <h1 class="page-header">Logs</h1>
          </div>
          <!-- end  page header -->
        </div>

        <div class="row">
          <!--  page header -->
          <div class="col-lg-12">
            <!-- Advanced Tables -->
            <div class="panel panel-default">
              <div class="panel-heading">
                visitor logs
              </div>
              <div class="panel-body">
                <div class="table-responsive">
                  <table
                    class="table table-striped table-bordered table-hover"
                    id="dataTables-example"
                  >
                    <thead>
                      <tr>
                        <th>Name</th>
                        <th>Company</th>
                        <th>Phone</th>
                        <th>Time in</th>
                        <th>Time out</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% for(log of result) {%>
                      <tr class="even gradeA">
                        <td><%= log.name %></td>
                        <td><%= log.company %></td>
                        <td><%= log.phone %></td>
                        <td class="center">
                          <%= new Date(log.timeIn).toISOString() %>
                        </td>
                        <td class="center">
                          <% if(log.timeOut){%> <%= new
                          Date(log.timeOut).toISOString() %><%} %>
                        </td>
                      </tr>
                      <% } %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
            <!--End Advanced Tables -->
          </div>
          <!-- end  page header -->
        </div>
      </div>
      <!-- end page-wrapper -->
    </div>
    <!-- end wrapper -->
    <!--modal-->
    <!-- Core Scripts - Include with every page -->

    <script src="/dashboard/plugins/jquery-1.10.2.js"></script>
    <script src="/dashboard/plugins/bootstrap/bootstrap.min.js"></script>
    <script src="/dashboard/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="/dashboard/plugins/pace/pace.js"></script>
    <script src="/dashboard/scripts/siminta.js"></script>
    <!-- Page-Level Plugin Scripts-->
    <script src="/dashboard/plugins/dataTables/jquery.dataTables.js"></script>
    <script src="/dashboard/plugins/dataTables/dataTables.bootstrap.js"></script>
    <script src="/report/jquery-3.3.1.js"></script>
    <script src="/report/jquery.dataTables.min.js"></script>
    <script src="/report/dataTables.buttons.min.js"></script>
    <script src="/report/buttons.flash.min.js"></script>
    <script src="/report/jszip.min.js"></script>
    <script src="/report/0.1.53/pdfmake.min.js"></script>
    <script src="/report/vfs_fonts.js"></script>
    <script src="/report/buttons.html5.min.js"></script>
    <script src="/report/buttons.print.min.js"></script>
    <script>
      $(document).ready(function() {
        $("#dataTables-example").DataTable({
          dom: "Bfrtip",
          buttons: ["copy", "csv", "excel", "pdf", "print"]
        });
      });
    </script>
  </body>
</html>
